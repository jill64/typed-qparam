      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "library",
    "querystring",
    "typesafe"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/typed-qparam.git",
    "image": "https://opengraph.githubassets.com/39a88c51cf0cf79ef10033876c54e5412ff8134c519b6f26bb485763350161ef/jill64/typed-qparam"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w .",
    "test": "vitest"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "2.0.20",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "dependencies": {
    "ts-serde": "1.0.9"
  }
}
